{
  "functions": {
    "chatgpt-3-main/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/chatgpt-3-main/(.*)", "dest": "/chatgpt-3-main/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "env": {
    "NOW_PHP_FOO": "bar3"
  },
  "build": {
    "env": {
      "NOW_PHP_DEBUG": "1"
    }
  }
}
